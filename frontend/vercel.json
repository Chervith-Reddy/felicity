{
  "rewrites": [
    {
      "source": "/api/:path*",
      "destination": "https://felicity-nual.onrender.com/api/:path*"
    },
    {
      "source": "/uploads/:path*",
      "destination": "https://felicity-nual.onrender.com/uploads/:path*"
    },
    {
      "source": "/((?!api|uploads).*)",
      "destination": "/index.html"
    }
  ]
}